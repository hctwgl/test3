<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ald.fanbei.api.dal.dao.AfFeedBackDao">


	<insert id="addFeedBack" parameterType="AfFeedBackDo" keyProperty="rid" useGeneratedKeys="true">
		INSERT INTO af_feedback
		(
			gmt_create,
			gmt_modified,
			user_id,
			content
		)
		VALUES
		(
			NOW(),
			NOW(),
			#{userId},
			#{content}
		)
	</insert>
	
</mapper>