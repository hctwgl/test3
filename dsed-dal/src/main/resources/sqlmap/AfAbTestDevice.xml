<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ald.fanbei.api.dal.dao.AfAbTestDeviceDao">

	
	<insert id="addUserDeviceInfo" parameterType="AfAbTestDeviceDo" keyProperty="id" useGeneratedKeys="true" >
		INSERT INTO
			af_abtest_device
			(
				gmt_create,
				gmt_modified,
				user_id,
				device_num
				
			)
		VALUES
		    (
				NOW(),
				NOW(),
				#{userId},
				#{deviceNum}
			)
	</insert>
</mapper>