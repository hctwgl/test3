<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ald.fanbei.api.dal.dao.AfMoblieChargeDao">
	<sql id="queryAllFields">
		id rid,province,company,price_json
	</sql>

	
	<select id="getMoblieChargeByTypeAndCompany" resultType="AfMoblieChargeDo">
		SELECT
			<include refid="queryAllFields" />
		FROM
			af_mobile_charge
		WHERE
			province = #{province}
		AND
			company = #{company}
	</select>
	
	


</mapper>