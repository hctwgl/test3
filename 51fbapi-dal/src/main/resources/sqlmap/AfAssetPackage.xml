     <select id="getCurrDayHaveGetTotalAmount" resultType="java.math.BigDecimal">
    	SELECT
			IFNULL(SUM(real_total_money),0)
		FROM
			af_asset_package
		WHERE
			gmt_create <![CDATA[ >= ]]> CURRENT_DATE()
