<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ald.fanbei.api.dal.dao.AfRedRainPoolDao">
	<sql id="queryFields">
		id,num,coupon_id,coupon_name,coupon_type,amount,gmt_create,gmt_modified,creator,modifier,is_delete
	</sql>
	
	<select id="queryAll" resultType="AfRedRainPoolDo">
		SELECT 
			<include refid="queryFields"/>
		FROM af_red_rain_pool WHERE is_delete = 0;
	</select>
	
</mapper>