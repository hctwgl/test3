<!DOCTYPE html>
<html lang="en">
<head>
    #set($version="201803051715")
    #set($commonUrl="https://f.51fanbei.com/h5/app/images/user/channel")
    <link rel="stylesheet" type="text/css" href="/dist/css/user/channelRegister.css?version=$version">
    #if( $!{style}==9)
        <title>51返呗-用不完注册</title>
    #else
        <title>51返呗注册</title>
    #end
</head>
<body>
<input type="hidden" id="channelCode" name="channelCode" value="$!{channelCode}">
<input type="hidden" id="pointCode" name="pointCode" value="$!{pointCode}">
<input type="hidden" id="style" value="$!{style}">
<div class="wrap" id="theme$!{style}">
    #if( $!{style}>=2 && $!{style}<=7||$!{style}==10)
        <marquee behavior="scroll" direction="up" scrollamount="1" class="mar">
            <p>用户131****8880 成功借款 8000元</p>
            <p>用户137****9767 成功借款 6000元</p>
            <p>用户186****2099 成功借款 7000元</p>
            <p>用户178****3863 成功借款 6000元</p>
            <p>用户187****9793 成功借款 4000元</p>
            <p>用户176****0389 成功借款 6000元</p>
            <p>用户157****3696 成功借款 5000元</p>
            <p>用户183****7197 成功借款 4000元</p>
            <p>用户137****0925 成功借款 7000元</p>
            <p>用户135****5276 成功借款 3000元</p>
            <p>用户187****0576 成功借款 6000元</p>
        </marquee>
    #end
    #if($!{style}==8 || $!{style}==17)
        <img src="$commonUrl/channel17txt.jpg">
    #end
    <img class="background" src="$commonUrl/channel$!{style}_01.jpg">
    #if( $!{style}==8 ||$!{style}==9 || $!{style}==16 || $!{style}==17 || $!{style}==12 || $!{style}==13|| $!{style}==14)
        <marquee behavior="scroll" direction="up" scrollamount="1" bgcolor="transparent" class="mar17">
            <p>用户131****8880 成功借款 8000元</p>
            <p>用户137****9767 成功借款 6000元</p>
            <p>用户186****2099 成功借款 7000元</p>
            <p>用户178****3863 成功借款 6000元</p>
            <p>用户187****9793 成功借款 4000元</p>
            <p>用户176****0389 成功借款 6000元</p>
            <p>用户157****3696 成功借款 5000元</p>
            <p>用户183****7197 成功借款 4000元</p>
            <p>用户137****0925 成功借款 7000元</p>
            <p>用户135****5276 成功借款 3000元</p>
            <p>用户187****0576 成功借款 6000元</p>
        </marquee>
    #end

    #if( $!{style}==6 )
        <div class="progress" id="progress">
            <div class="text">￥20000</div>
            <div class="progress_bg">
                <div class="progress_bar"></div>
            </div>
            <div class="progress_btn" id="progress_btn"></div>
            <div class="moneyFlag">
                <span class="lf" id="leftMoney">￥500</span>
                <span class="fr" id="rightMoney">￥0.5</span>
            </div>
        </div>
    #end
    <div class="form">
        <img class="background" src="$commonUrl/channel$!{style}_02.jpg">
        <ul class="iform clearfix">
            <li><input type="text" id="register_mobile" pattern="[0-9]*" maxlength="11" placeholder="请输入注册手机号" onfocus="toMaidian('phone')"></li>

            #if($!{style}==1||$!{style}==8||$!{style}==9||$!{style}==17||$!{style}==12||$!{style}==13||$!{style}==14)
                #if($!{style}!=8&&$!{style}!=9&&$!{style}!=12&&$!{style}!=13&&$!{style}!=14)
                    <li><input type="password" id="register_password" placeholder="密码长度为6-18位数字和字母" name="words" maxlength="18" onfocus="toMaidian('pwd')"></li>
                #end
                <li class="clearfix">
                    <input type="text" id="register_verification" maxlength="6" pattern="[0-9]*" placeholder="请输入验证码" onfocus="toMaidian('code')">
                    <button id="register_codeBtn" onclick="toMaidian('codeBtn')">点击获取</button>
                </li>
                <li><button id="register_submitBtn"></button></li>
                <div class="clearfix register_checkbox">
                    <input type="checkbox" class="input_check" id="input_check" checked="checked">
                    <label for="input_check"></label>
                    <p class="clearfix fontColor"><span>阅读并同意</span><a href="/fanbei-web/app/protocolRegister" class="fontColor">《51返呗用户注册协议》</a></p>
                </div>
                #if($!{style}!=12&&$!{style}!=13&&$!{style}!=14)
                <p class="logintxt clearfix" style=" margin: 0 auto;font-size: 0.28rem;">
                    <span class="fontColor" style="float:left;margin-right:0.1rem;">已注册用户</span>
                    <a class="fontColor" href="http://a.app.qq.com/o/simple.jsp?pkgname=com.alfl.www" style="float:left;text-decoration:underline;" onclick="toMaidian('toLogin')">直接登录</a>
                </p>
                 #else
                    <a class="tologin" href="http://a.app.qq.com/o/simple.jsp?pkgname=com.alfl.www" onclick="toMaidian('toLogin')"></a>
                #end
            #elseif( $!{style}==16 ||$!{style}==7)
                <li><input type="password" id="register_password" placeholder="密码长度为6-18位数字和字母" name="words" maxlength="18" onfocus="toMaidian('pwd')"></li>
                <li class="clearfix">
                    <input type="text" id="register_verification" maxlength="6" pattern="[0-9]*" placeholder="请输入验证码" onfocus="toMaidian('code')">
                    <button id="register_codeBtn" onclick="toMaidian('codeBtn')">点击获取</button>
                </li>
                <li><button id="register_submitBtn"></button></li>
                <div class="clearfix register_checkbox">
                    <input type="checkbox" class="input_check" id="input_check" checked="checked">
                    <label for="input_check"></label>
                    <p class="clearfix fontColor"><span>阅读并同意</span><a href="/fanbei-web/app/protocolRegister" class="fontColor">《51返呗用户注册协议》</a></p>
                </div>
            #else
                <li class="clearfix">
                    <input type="text" id="register_verification" maxlength="6" pattern="[0-9]*" placeholder="请输入验证码" onfocus="toMaidian('code')">
                    <button id="register_codeBtn" onclick="toMaidian('codeBtn')">点击获取</button>
                </li>
                #if($!{style}!=10&&$!{style}!=11)
                    <li><input type="password" id="register_password" placeholder="密码长度为6-18位数字和字母" name="words" maxlength="18" onfocus="toMaidian('pwd')"></li>
                #end
                <li><button id="register_submitBtn"></button></li>
                <div class="clearfix register_checkbox">
                    <input type="checkbox" class="input_check" id="input_check" checked="checked">
                    <label for="input_check"></label>
                    <p class="clearfix fontColor"><span>阅读并同意</span><a href="/fanbei-web/app/protocolRegister" class="fontColor">《用户协议》</a></p>
                </div>
                <p class="logintxt clearfix" style="">
                    <span class="fontColor" style="float:left;margin-right:0.1rem;">已注册?</span>
                    <a class="fontColor" href="http://a.app.qq.com/o/simple.jsp?pkgname=com.alfl.www" style="float:left;color:#ff5b56" onclick="toMaidian('toLogin')">点我马上登录</a>
                </p>
            #end

        </ul>
    </div>
    #if( $!{style}==1 )
        <div class="copyright">
            版权所有：$!{copyright} 如有疑问，请联系在线客服或拨打0571-86802133
            <br>
        </div>
    #else
        <img src="$commonUrl/channel$!{style}_03.jpg">
        <a href="tel:4000025151" style="position: absolute;bottom:0;height:1rem;width:7.5rem"></a>
    #end
    #if( $!{style}==12 || $!{style}==13|| $!{style}==14)
        <img class="goTop" src="$commonUrl/addRegister06.png">
    #end
</div>
<script type="text/javascript" src="https://img.51fanbei.com/h5/static/core.js"></script>
<script type="text/javascript" src="https://img.51fanbei.com/h5/static/md5.js"></script>
<script type="text/javascript" src="https://img.51fanbei.com/h5/static/gt.js"></script>
<script type="text/javascript" src="/dist/js/user/channelRegister.js?version=$version"></script>
<div style="display:none">
    <script src="https://s4.cnzz.com/z_stat.php?id=1261759898&web_id=1261759898" language="JavaScript"></script>
</div>
</body>
</html>
