      </div>
      <div class="content">
        <div class="con_tit"></div>
        <div class="con"></div>
      </div>
    </div>
    <div class="bottom"></div>
  </div>
<script type="text/javascript" src="/dist/js/app/newinvite.js?version=$version"></script>
