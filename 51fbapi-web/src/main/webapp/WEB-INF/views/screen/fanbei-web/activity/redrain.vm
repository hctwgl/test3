      <div class="light _light-left"></div>
      <div class="light _light-right"></div>
    </div>
    <div class="pre2"></div>
    <div class="pre3">
      <div class="count">
        <div class="hour">00</div>
        <div class="minute">00</div>
        <div class="second">00</div>
      </div>
    </div>
    <div class="pre4">
      <div class="entry">
        <div class="_entry fir">
          <div>
