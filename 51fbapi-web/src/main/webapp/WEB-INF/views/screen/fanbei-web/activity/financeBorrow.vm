	<img src="$commonUrl/financeBorrow3.jpg">
	<div class="financeBorrowBtn">
		<img src="$commonUrl/financeBorrow4.png" @click="goBorrowMoney">
	</div>
	<script type="text/javascript">
		new Vue({
			el:'.financeBorrowBtn',
			methods:{
				goBorrowMoney: function(){ // 跳转借钱
					window.location.href = '/fanbei-web/opennative?name=BORROW_MONEY';
				}
			}
		});
