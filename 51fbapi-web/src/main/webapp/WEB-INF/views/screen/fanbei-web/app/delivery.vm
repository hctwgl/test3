<!DOCTYPE html>
<html>
<head>
    #set($version="201708102130")
    #set($commonUrl="http://f.51fanbei.com/h5")
    <title>物流信息</title>
    <link rel="stylesheet" type="text/css" href="/dist/css/app/delivery.css?version=$version">
</head>

<body style="background: #ededed">
<div id="delivery">
    <div id="top">
        <ul>
           <span class="state">
                <span class="stateTitle">物流状态</span>
                <template v-if="stateDesc && stateDesc !=''">
                      <span class="stateDesc">{{stateDesc}}</span>
                </template>
            </span>
        </ul>
        <ul>
            <span class="from">
                <span class="fromTitle">承运来源:</span>
                <template v-if="shipperName && shipperName !=''">
                      <span class="shipperName">{{shipperName}}</span>
                </template>
        </span>
        </ul>
        <ul>
            <span class="no">
                <span class="noTitle">运单编号:</span>
                <template v-if="shipperCode && shipperCode !=''">
                      <span class="shipperCode">{{shipperCode}}</span>
                </template>
            </span>
        </ul>

    </div>

    <div id="list" class="main_wrap listMainContent">
        <template v-if="returnData && returnData !=''">
            <ul v-for="(item,index) in returnData" class="item">
                <li class="comRank rank1" v-if="index ==0">
                    <span></span>
                </li>
                <li class="comRank rank2" v-else-if="index ==1">
                    <span></span>
                </li>
                <li class="comRank rank3" v-else-if="index ==2">
                    <span></span>
                </li>
                <li class="rank" v-else>
                    <span>{{index+1}}</span>
                </li>
                <li class="account">
                    <span>{{formateTelNum(item.user_name)}}</span>
                </li>
                <li class="amount">
                    <span>{{item.sum_money}}</span>
                </li>
            </ul>
        </template>
    </div>
</div>
<script type="text/javascript" src="/dist/js/app/delivery.js?version=$version"></script>
</body>
</html>

