<!DOCTYPE html>
<html>
<head>
    #set($version="201708102130")
    #set($commonUrl="http://f.51fanbei.com/h5")
    <title>全国排行榜</title>
    <link rel="stylesheet" type="text/css" href="/dist/css/app/inviteRank.css?version=$version">
</head>

<body style="background: linear-gradient(to bottom,#fd5d2c, #ff4a2d);">
<div id="inviteRank">
    <div id="top">
        <span class="topTitle">7月Top20</span>
        <span class="divider"><i></i></span>
    </div>
    <div id="navWrap">
        <ul class="nav">
            <li id="first">
                <span>排行榜</span>
            </li>
            <li id="second">
                <span>用户</span>
            </li>
            <li id="third">
                <span>奖励/元</span>
            </li>
        </ul>
    </div>

    <div id="list" class="main_wrap listMainContent">
        <template v-if="returnData && returnData !=''">
            <ul v-for="(item,index) in returnData" class="item">
                <li class="comRank rank1" v-if="index ==0">
                    <span></span>
                </li>
                <li class="comRank rank2" v-else-if="index ==1">
                    <span></span>
                </li>
                <li class="comRank rank3" v-else-if="index ==2">
                    <span></span>
                </li>
                <li class="rank" v-else>
                    <span>{{index+1}}</span>
                </li>
                <li class="account">
                    <span>{{formateTelNum(item.user_name)}}</span>
                </li>
                <li class="amount">
                    <span>{{item.sum_money}}</span>
                </li>
            </ul>
        </template>
    </div>
</div>
<script type="text/javascript" src="/dist/js/app/inviteRank.js?version=$version"></script>
</body>
</html>

