<!DOCTYPE html>
<html>
<head>
    #set($version="201708102130")
    #set($commonUrl="http://f.51fanbei.com/h5")
    <title>全国排行榜</title>
    <link rel="stylesheet" type="text/css" href="/dist/css/app/inviteRank.css?version=$version">
</head>

<body style="background-color:#ff0000;">
<div id="inviteRank">
    <div id="top">
        <span>7月Top20</span>
    </div>
    <div id="navWrap">
        <ul class="nav">
            <li id="first">
                <span>排行榜</span>
            </li>
            <li id="second">
                <span>用户</span>
            </li>
            <li id="third">
                <span>奖励/元</span>
            </li>
        </ul>
    </div>

    <div id="list" class="main_wrap listMainContent">
        <ul v-for="(item,index) in returnData" class="item">
            <li class="rank" v-if="index ==0">
                <span>1</span>
            </li>
            <li class="rank" v-else-if="index ==1">
                <span>2</span>
            </li>
            <li class="rank" v-else-if="index ==2">
                <span>3</span>
            </li>
            <li class="rank" v-else>
                <span>{{index+1}}</span>
            </li>
            <li class="account">
                <span>{{item.user_name}}</span>
            </li>
            <li class="amount">
                <span>{{item.sum_money}}</span>
            </li>
        </ul>
    </div>
</div>
<script type="text/javascript" src="/dist/js/app/inviteRank.js?version=$version"></script>
</body>
</html>

