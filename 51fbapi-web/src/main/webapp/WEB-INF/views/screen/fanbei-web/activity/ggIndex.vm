    <div class="banner">轮播图</div>
    <div class="couponList clearFix">
        <div class="coupon">
            <p class="name">易果生鲜</p>
            <p class="amount">￥<span>3.0</span></p>
            <p class="limitAmount white">满<span>10</span>元可用</p>
