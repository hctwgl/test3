      </marquee>
      <img class="bg" src="$commonUrl/reg2.jpg">
      <img class="bg" src="$commonUrl/reg3.jpg">
      <div class="inter">
        <input type="tel" maxlength="11" placeholder="请输入手机号" id="user" />
        <input type="password" maxlength="18" placeholder="请输入密码" id="pwd" />
        <div class="verify">
          <input id="verify" type="text" maxlength="6" placeholder="输入验证码"/>
          <div class="getcode">获取验证码</div>
        </div>
        <div class="reg"></div>
        <div class="agree">
          <div class="checkbox checked"></div>
          <span>已阅读并同意</span><a href="/fanbei-web/app/protocolRegister">《51返呗用户注册协议》</a>
        </div>
        <p class="registered clearfix">
          <a href="http://a.app.qq.com/o/simple.jsp?pkgname=com.alfl.www">已有账号登录</a>
        </p>
      </div>
  </div>

  <div class="registerMask hide"></div>
  <div class="imgVftCodeWrap hide">
    <div class="imgVftCodeTitle"><p>请输入图形验证码</p><span id="imgVftCodeClose"><i></i></span></div>
    <div class="imgVftCodeInput clearfix">
        <div class="imgVftCodeDiv"><input type="text" class="imgVftCode" id="imgVftCode" maxlength="4" placeholder="请输入"></div>
        <div class="imgVftCodeWrapImgDiv"><img id="imgVftCodeWrapImg" src=""></div>
      <span class="imgVftCodeRefresh"><i></i></span>
    </div>
    <button class="imgVftCodeSbumit">确定</button>
  </div>
  <script type="text/javascript" src="/dist/js/core.js"></script>
  <script type="text/javascript" src="/dist/js/md5.js"></script>
